var = int(input("Input a integer."))

def fbzz(multiple):
    global var
    return var % multiple == 0

try:
    if fbuzz(3) and fbuzz(5):
        print("FizzBuzz")
    elif fbuzz(3):
        print("Fizz")
    elif fbuzz(5):
        print("Buzz")
    else:
        print(var)
    
except:
    print("NotIntegerError: input a INTEGER. Other kinds of variable-type are invalid.")

  
